{
  "name": "ex-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "build:app.dev.ios": "webpack --config ./config/webpack.app.dev.js --progress --platform ios && cordova run ios --device",
    "build:app.prod.ios": "webpack --config ./config/webpack.app.prod.js --progress --platform ios && cordova build ios --release --device",
    "build:app.dev.android": "webpack --config ./config/webpack.app.dev.js --progress --platform android && cordova run android",
    "build:app.prod.android": "webpack --config ./config/webpack.app.prod.js --progress --platform android && cordova build android --release",
    "build:web.local": "webpack --config ./config/webpack.web.local.js --progress",
    "build:web.prod": "webpack --config ./config/webpack.web.prod.js --progress",
    "serve:web.local": "webpack-dev-server --config ./config/webpack.web.local.js --progress --inline --hot",
    "serve:web.prod": "http-server dist --cors",
    "start": "npm run serve:web.local",
    "test": "karma start",
    "test-vsts": "karma start karma.vsts.conf.js",
    "pree2e": "concurrently \" tsc -p e2e/ \" \"npm run build:web.local\" ",
    "e2e": "concurrently \"npm run serve:web.prod\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js"
  },
